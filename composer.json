{
    "name": "timothepearce/laravel-cargo",
    "description": "This is my package LaravelCargo",
    "keywords": [
        "LaravelCargo",
        "laravel",
        "laravel-cargo"
    ],
    "homepage": "https://github.com/timothepearce/laravel-cargo",
    "license": "MIT",
    "authors": [
        {
            "name": "Timoth√© Pearce",
            "email": "timothe.pearce@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Laravelcargo\\LaravelCargo\\": "src",
            "Laravelcargo\\LaravelCargo\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelcargo\\LaravelCargo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelcargo\\LaravelCargo\\LaravelCargoServiceProvider"
            ],
            "aliases": {
                "LaravelCargo": "Laravelcargo\\LaravelCargo\\LaravelCargoFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
